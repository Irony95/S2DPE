      <TextField fx:id="tfSaveSceneName" layoutX="16.0" layoutY="97.0" promptText="file name" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="100.0" />
      <Button layoutX="173.0" layoutY="97.0" mnemonicParsing="false" onAction="#onSaveScene" text="Save Scene" AnchorPane.topAnchor="100.0" />
      <Button layoutX="268.0" layoutY="100.0" mnemonicParsing="false" onAction="#onLoadScene" text="Load Scene" AnchorPane.rightAnchor="48.0" AnchorPane.topAnchor="100.0" />
   </children>
</AnchorPane>
