      <TextField fx:id="tfTimeStepSize" layoutX="238.0" layoutY="24.0" prefHeight="25.0" prefWidth="73.0" promptText="time step" text="0.016666" AnchorPane.rightAnchor="90.0" />
      <CheckBox fx:id="cbApplyFriction" layoutX="21.0" layoutY="61.0" mnemonicParsing="false" onAction="#onApplyFrictionInteracted" selected="true" text="Apply friction" AnchorPane.leftAnchor="10.0" />
      <TextField fx:id="tfAirDensity" layoutX="238.0" layoutY="61.0" onAction="#onAirDensityChanged" prefHeight="25.0" prefWidth="73.0" promptText="time step" text="1.293" AnchorPane.rightAnchor="90.0" />
      <Label layoutX="321.0" layoutY="65.0" text="Air density" AnchorPane.rightAnchor="10.0" />
