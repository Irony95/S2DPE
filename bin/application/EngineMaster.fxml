<AnchorPane minHeight="500.0" minWidth="300.0" prefHeight="700.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EngineMasterController">
   <children>
      <Button fx:id="btnPausePlay" layoutX="23.0" layoutY="24.0" mnemonicParsing="false" onAction="#onPlayClicked" prefHeight="31.0" prefWidth="65.0" text="Play" AnchorPane.leftAnchor="10.0" />
