      <Line endX="1000.0" layoutX="101.0" layoutY="135.0" startX="-100.0" stroke="#7b7b7b" strokeWidth="2.0" />
      <Button layoutX="10.0" layoutY="458.0" mnemonicParsing="false" text="Place" AnchorPane.leftAnchor="10.0" />
      <TabPane fx:id="tpSceneLists" layoutX="-5.0" layoutY="533.0" prefHeight="227.0" prefWidth="400.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="500.0">
        <tabs>
          <Tab text="Entities">
               <content>
                  <ListView fx:id="lvEntities" prefHeight="132.0" prefWidth="400.0" />
               </content>
            </Tab>
          <Tab text="Forces">
               <content>
                  <ListView fx:id="lvForces" prefHeight="200.0" prefWidth="200.0" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <Label layoutX="10.0" layoutY="429.0" text="Material" AnchorPane.leftAnchor="10.0" />
      <ComboBox fx:id="cobMaterial" layoutX="90.0" layoutY="424.0" prefWidth="150.0" promptText="Simple Material" AnchorPane.leftAnchor="90.0" />
      <Button layoutX="296.0" layoutY="458.0" mnemonicParsing="false" onAction="#startMaterialWindow" text="New Material" AnchorPane.rightAnchor="10.666666666666686" />
      <Line endX="1000.0" layoutX="101.0" layoutY="375.0" startX="-100.0" stroke="#7b7b7b" strokeWidth="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="500.0" />
      <Button fx:id="fxDeleteButton" layoutX="11.0" layoutY="564.0" mnemonicParsing="false" onAction="#onDeleteClicked" text="Delete Selected" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="10.0" />
